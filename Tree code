
class Node:
    
    def __init__ (self, data):
        
        self.left = None
        self.right = None
        self.data = data

    def printTree (self):
        
        print(self.data)
        
    def insert (self, data):
        
        if self.data:
            
            if data:
                
                if self.right is None:
                    
                    self.right = Node(data)
                    
        else:
            
            self.data = data
    


root = Node(10)

root.insert(6)
root.printTree()




